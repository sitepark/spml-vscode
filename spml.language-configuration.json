{
	"comments": {
		"blockComment": ["<%--", "--%>"]
	},
	"brackets": [["[", "]"], ["(", ")"]],
	"autoClosingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["'", "'"],
		["\"", "\""]
	],
	"surroundingPairs": [
		["'", "'"],
		["\"", "\""],
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["<", ">"],
		["<%", "%>"]
	],
	"folding": {
		"markers": {
			"start": "^\\s*<\\%--\\s*#region\\b.*--\\%>",
			"end": "^\\s*<\\%--\\s*#endregion\\b.*--\\%>"
		}
	},
	"wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\$\\^\\&\\*\\(\\)\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\s]+)"
}
